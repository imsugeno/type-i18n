{
  "name": "type-i18n",
  "version": "0.1.0",
  "description": "Type-safe internationalization library for React with full TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
